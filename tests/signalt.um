
import (
	"th.um"
	"rect.um"
	"input.um"
	"signal.um"
	"canvas.um"
	"window.um"
)

fn main() {
	window.setup("title", 1280, 720)
	window.setViewport(th.Vf2{192, 108})

	signal.register("spacePress", signal.Callback{
		printf("%s\n", repr(ctx))
	})

	for window.cycle() {
		if input.isPressed(' ') {
			signal.emit("spacePress", th.time)
		}
	}
}
